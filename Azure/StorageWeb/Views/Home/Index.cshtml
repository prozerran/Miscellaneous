@model IEnumerable<StorageWeb.Models.BlobImage>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    
    @foreach (var item in Model) {
        <img src="@item.BlobUri" />
    }

</body>
</html>
